GITHUB_APP_ID=
GITHUB_APP_PRIVATE_KEY="-----BEGIN PRIVATE KEY-----\n...your key...\n-----END PRIVATE KEY-----"
GITHUB_WEBHOOK_SECRET=

# GitHub OAuth for web dashboard
GITHUB_CLIENT_ID=
GITHUB_CLIENT_SECRET=
GITHUB_REDIRECT_URI=http://localhost:3000/api/auth/github/callback

# JWT secret for user authentication
JWT_SECRET=your-secret-key-for-jwt-tokens

# Database Configuration (Docker Compose will override these)
DATABASE_URL=postgresql://user:pass@host:5432/gitautonomic
REDIS_URL=redis://localhost:6379

# Docker Port Configuration (optional, defaults provided)
PORT=3000
DB_PORT=5433
REDIS_PORT=6379

# AI Configuration
OPENAI_API_KEY=
CUSTOM_LLM_ENDPOINT=
CUSTOM_LLM_API_KEY=
EMBEDDINGS_MODEL=text-embedding-3-small

# Agent Configuration
RISK_HIGH_THRESHOLD=0.7
COVERAGE_MIN_LINES=0.75
AGENT_WORK_ROOT=/tmp/ai-agent-work

# Application Configuration
LOG_LEVEL=info
NODE_ENV=production
